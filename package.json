{
  "name": "mes-mobile-app",
  "version": "0.0.1",
  "author": "DVSA",
  "description": "DVSA Mobile Examiner Services (GDS Alpha phase)",
  "homepage": "https://github.com/dvsa/mes-mobile-app",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "bootstrap": "json2ts schema/schemaExaminerSchedule.json > src/shared/models/DJournal.ts",
    "build": "ionic-app-scripts build",
    "compile-no-emit": "tsc --noEmit -p .",
    "device": "ionic cordova run ios --device --development --buildConfig buildScripts/build.json",
    "lint": "ionic-app-scripts lint",
    "lint:fix": "tslint --fix -p ./",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "serve:local": "run-s update-mock-data copy-mock-data config:local ionic:serve",
    "ionic:deploy": "git push ionic master",
    "format:all": "npx prettier --write \"src/**/*{.ts,.js,.json,.css,.scss}\"",
    "prepush": "run-s lint test gitSecrets scanRepo",
    "precommit": "run-s lint:fix lint compile-no-emit",
    "test": "npm run update-mock-data && npm run test:jasmine",
    "test:jasmine": "karma start ./test-config/karma.conf.js",
    "test:jasmine-ci": "karma start ./test-config/karma.conf.js --single-run",
    "test:jasmine-coverage": "karma start ./test-config/karma.conf.js --coverage",
    "test:e2e-simulator-bdd": "protractor e2e-simulator-bdd.conf.js",
    "test:generate-report": "node generateTestReport.js",
    "gitSecrets": "git secrets --scan",
    "scanRepo": "git log -p -n 15 | scanrepo",
    "stylelint": "stylelint 'src/**/*.scss' fix --config stylelint.json",
    "config:dev": "cp src/environment/environment.dev.ts src/environment/environment.ts",
    "config:perf": "cp src/environment/environment.perf.ts src/environment/environment.ts",
    "config:uat": "cp src/environment/environment.uat.ts src/environment/environment.ts",
    "config:staging": "cp src/environment/environment.staging.ts src/environment/environment.ts",
    "config:production": "cp src/environment/environment.production.ts src/environment/environment.ts",
    "config:local": "cp src/environment/environment.local.ts src/environment/environment.ts",
    "copy-mock-data": "cp -r mock/ src/assets/mock/",
    "update-mock-data": "ts-node -P tsconfig.scripts.json bin/update-mock-data.ts"
  },
