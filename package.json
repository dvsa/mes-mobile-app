{
  "name": "mobile-examiner-alpha",
  "version": "0.0.1",
  "description": "DVSA Mobile Examiner Services (GDS Alpha phase)",
  "author": "DVSA",
  "homepage": "https://github.com/dvsa/mobile-examiner-alpha",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "device": "ionic cordova run ios --device --development --buildConfig buildScripts/build.json",
    "lint": "ionic-app-scripts lint",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "ionic:deploy": "git push ionic master",
    "format:fix": "pretty-quick --staged",
    "format:all": "npx prettier --write \"src/**/*{.ts,.js,.json,.css,.scss}\"",
    "precommit": "run-s format:fix lint",
    "prepush": "run-s format:fix lint test gitSecrets scanRepo",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:e2e-browser-bdd": "protractor e2e-browser-bdd.conf.js",
    "test:e2e-simulator-bdd": "protractor e2e-simulator-bdd.conf.js",
    "test:generate-report": "node generateTestReport.js",
    "gitSecrets": "git secrets --scan",
    "scanRepo": "git log -p -n 15 | scanrepo"
  },
  "dependencies": {
    "@angular-redux/store": "^7.1.1",
    "@angular/common": "5.2.10",
    "@angular/compiler": "5.2.10",
    "@angular/core": "5.2.10",
    "@angular/forms": "5.2.10",
    "@angular/http": "5.2.10",
    "@angular/platform-browser": "5.2.10",
    "@angular/platform-browser-dynamic": "5.2.10",
    "@ionic-native/core": "4.4.0",
    "@ionic-native/device": "^4.17.0",
    "@ionic-native/file": "^4.7.0",
    "@ionic-native/globalization": "^4.7.0",
    "@ionic-native/google-analytics": "^4.17.0",
    "@ionic-native/http": "^4.6.0",
    "@ionic-native/insomnia": "^4.7.0",
    "@ionic-native/media": "^4.7.0",
    "@ionic-native/screen-orientation": "^4.7.0",
    "@ionic-native/splash-screen": "4.4.0",
    "@ionic-native/status-bar": "4.4.0",
    "@ionic/storage": "2.1.3",
    "angular2-signaturepad": "^2.8.0",
    "cordova-browser": "^5.0.4",
    "cordova-ios": "4.5.5",
    "cordova-plugin-advanced-http": "^1.11.1",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-device-authentication": "git+https://github.com/dvsa/cordova-plugin-device-authentication.git",
    "cordova-plugin-file": "^6.0.1",
    "cordova-plugin-globalization": "^1.11.0",
    "cordova-plugin-google-analytics": "1.8.6",
    "cordova-plugin-insomnia": "^4.3.0",
    "cordova-plugin-ionic-keyboard": "^2.1.3",
    "cordova-plugin-ionic-webview": "^2.2.3",
    "cordova-plugin-media": "^5.0.2",
    "cordova-plugin-screen-orientation": "^3.0.1",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-statusbar": "^2.4.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "event-stream": "^3.3.4",
    "flux-standard-action": "^2.0.1",
    "govuk_frontend_toolkit": "^7.4.2",
    "ionic-angular": "3.9.2",
    "ionicons": "3.0.0",
    "lodash": "^4.17.5",
    "moment": "^2.22.2",
    "ng2-translate": "^5.0.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "rxjs": "5.5.2",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@angular/compiler-cli": "5.2.10",
    "@ionic/app-scripts": "^3.2.0",
    "@types/jest": "^22.2.2",
    "@types/lodash": "^4.14.106",
    "@types/node": "^9.6.0",
    "appium": "^1.9.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "cucumber": "^4.2.1",
    "cucumber-html-reporter": "^4.0.2",
    "dotenv-safe": "^5.0.1",
    "git-rev-sync": "^1.12.0",
    "husky": "^0.14.3",
    "ionic-mocks-jest": "^1.3.3",
    "jest": "^22.4.2",
    "jest-preset-angular": "^5.2.1",
    "ng-mocks": "^7.1.2",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.11.1",
    "pretty-quick": "^1.4.1",
    "protractor": "^5.3.0",
    "protractor-cucumber-framework": "^5.0.0",
    "ts-node": "^5.0.1",
    "tslint-config-airbnb": "^5.8.0",
    "tslint-config-prettier": "^1.10.0",
    "typescript": "2.4.2",
    "xml2js": "^0.4.19"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-ionic-keyboard": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-advanced-http": {},
      "cordova-plugin-screen-orientation": {},
      "cordova-plugin-insomnia": {},
      "cordova-plugin-device-authentication": {},
      "cordova-plugin-globalization": {},
      "cordova-plugin-file": {},
      "cordova-plugin-media": {
        "KEEP_AVAUDIOSESSION_ALWAYS_ACTIVE": "NO"
      },
      "cordova-plugin-google-analytics": {}
    },
    "platforms": [
      "browser",
      "ios"
    ]
  }
