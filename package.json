{
  "name": "mes-mobile-app",
  "version": "0.0.1",
  "author": "DVSA",
  "description": "DVSA Mobile Examiner Services (GDS Alpha phase)",
  "homepage": "https://github.com/dvsa/mes-mobile-app",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "bootstrap": "json2ts schema/schemaExaminerSchedule.json > src/common/domain/DJournal.ts",
    "build": "ionic-app-scripts build",
    "device": "ionic cordova run ios --device --development --buildConfig buildScripts/build.json",
    "lint": "ionic-app-scripts lint",
    "lint:fix": "tslint --fix -p ./",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "serve:local": "run-s copy-mock-data config:local ionic:serve",
    "ionic:deploy": "git push ionic master",
    "format:fix": "pretty-quick --staged",
    "format:all": "npx prettier --write \"src/**/*{.ts,.js,.json,.css,.scss}\"",
    "prepush": "run-s format:fix lint test gitSecrets scanRepo",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:e2e-browser-bdd": "protractor e2e-browser-bdd.conf.js",
    "test:e2e-simulator-bdd": "protractor e2e-simulator-bdd.conf.js",
    "test:generate-report": "node generateTestReport.js",
    "gitSecrets": "git secrets --scan",
    "scanRepo": "git log -p -n 15 | scanrepo",
    "stylelint": "stylelint 'src/**/*.scss' fix --config stylelint.json",
    "config:dev": "cp src/environment/environment.dev.ts src/environment/environment.ts",
    "config:uat": "cp src/environment/environment.uat.ts src/environment/environment.ts",
    "config:staging": "cp src/environment/environment.staging.ts src/environment/environment.ts",
    "config:production": "cp src/environment/environment.production.ts src/environment/environment.ts",
    "config:local": "cp src/environment/environment.local.ts src/environment/environment.ts",
    "copy-mock-data": "cp -r mock/ src/assets/mock/"
  },
  "dependencies": {
    "@angular/animations": "5.2.11",
    "@angular/common": "5.2.11",
    "@angular/compiler": "5.2.11",
    "@angular/compiler-cli": "5.2.11",
    "@angular/core": "5.2.11",
    "@angular/forms": "5.2.11",
    "@angular/http": "5.2.11",
    "@angular/platform-browser": "5.2.11",
    "@angular/platform-browser-dynamic": "5.2.11",
    "@ionic-native/core": "~4.17.0",
    "@ionic-native/in-app-browser": "^4.18.0",
    "@ionic-native/ms-adal": "^4.18.0",
    "@ionic-native/screen-orientation": "^4.19.0",
    "@ionic-native/splash-screen": "~4.17.0",
    "@ionic-native/status-bar": "~4.17.0",
    "@ionic/storage": "2.2.0",
    "@ngrx/effects": "^5.2.0",
    "@ngrx/store": "^5.2.0",
    "@types/jwt-decode": "^2.2.1",
    "aws-sdk": "^2.382.0",
    "aws4": "^1.8.0",
    "cordova-ios": "4.5.5",
    "cordova-plugin-compat": "1.2.0",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-inappbrowser": "3.0.0",
    "cordova-plugin-ionic-keyboard": "^2.1.3",
    "cordova-plugin-ionic-webview": "^3.0.0",
    "cordova-plugin-ms-adal": "0.10.1",
    "cordova-plugin-screen-orientation": "3.0.1",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-statusbar": "^2.4.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "deep-diff": "^1.0.2",
    "event-stream": "3.3.4",
    "ionic-angular": "3.9.2",
    "ionicons": "3.0.0",
    "moment": "^2.23.0",
    "jwt-decode": "^2.2.0",
    "rxjs": "^5.5.11",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@ionic/app-scripts": "3.2.1",
    "@ngrx/store-devtools": "^5.2.0",
    "@types/aws-sdk": "^2.7.0",
    "@types/aws4": "^1.5.1",
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.10",
    "appium": "^1.10.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cucumber": "^5.0.2",
    "cucumber-html-reporter": "^4.0.4",
    "dotenv-safe": "^6.1.0",
    "git-rev-sync": "^1.12.0",
    "husky": "^1.2.0",
    "ionic-mocks-jest": "^1.3.3",
    "jest": "^23.6.0",
    "jest-preset-angular": "^6.0.1",
    "json-schema-to-typescript": "^6.1.0",
    "ng-mocks": "^7.1.3",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.15.2",
    "pretty-quick": "^1.8.0",
    "protractor": "^5.4.1",
    "protractor-cucumber-framework": "^6.1.1",
    "request": "^2.88.0",
    "rxjs-tslint-rules": "^4.14.3",
    "stylelint": "^9.9.0",
    "stylelint-config-standard": "^18.2.0",
    "ts-node": "^7.0.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "~2.6.2",
    "xml2js": "^0.4.19"
  },
  "platforms": [
    "browser",
    "ios"
  ],
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-keyboard": {},
      "cordova-plugin-ms-adal": {},
      "cordova-plugin-inappbrowser": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-screen-orientation": {}
    },
    "platforms": [
      "ios"
    ]
  }
}